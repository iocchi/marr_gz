<?xml version="1.0"?>
<robot name="wheeled_robot" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:arg name="namespace" default="" />
  <xacro:arg name="use_imu" default="false" />
  <xacro:arg name="use_lidar" default="false" />
  <xacro:arg name="use_rgb" default="false" />
  <xacro:arg name="use_rgbd" default="false" />
  <xacro:arg name="use_arms" default="false" />
  <xacro:arg name="use_pantilt" default="false" />

  <xacro:include filename="$(find marr_gz)/urdf/inertia.xacro" />
  <xacro:include filename="$(find marr_gz)/urdf/sensors.xacro" />
  <xacro:include filename="$(find marr_gz)/urdf/arms.xacro" />
  <xacro:include filename="$(find marr_gz)/urdf/pantilt.xacro" />

 
  <xacro:property name="friction" value="0.01" />
  <xacro:property name="damping" value="0.05" />
  <xacro:property name="max_effort" value="1000.0" />
  <xacro:property name="max_velocity" value="10.0" />

  <material name="orange"><color rgba="1.0 0.33 0.0 1.0"/></material>
  <material name="red"><color rgba="0.8 0 0 1"/></material>
  <material name="green"><color rgba="0 1 0 1"/></material>
  <material name="blue"><color rgba="0 0 1 1"/></material>
  <material name="black"><color rgba="0 0 0 1.0"/></material>
  <material name="grey"><color rgba="0.7 0.7 0.7 1.0"/></material>
  <material name="white"><color rgba="1 1 1 1.0"/></material>


  <link name="base_footprint"/>

  <link name="base_link">
    <visual>
        <geometry>  
            <box size="0.5 0.4 0.3"/> 
        </geometry>   
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> 
        <material name="red"/>
    </visual>
    <collision>
        <geometry>  
            <box size="0.5 0.4 0.3"/> 
        </geometry>   
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/> 
    </collision>
    <xacro:box_inertial mass="0.2" x_dim="0.5" y_dim="0.4" z_dim="0.3"/>
  </link>

  <link name="left_wheel">
    <visual>
        <geometry>
            <cylinder radius="0.1" length="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="1.57 0.0 0.0"/>
        <material name="black"/>     
    </visual>
    <collision>
        <geometry>
            <cylinder radius="0.1" length="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="1.57 0.0 0.0"/>
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
            <torsional>
              <ode/>
            </torsional>
          </friction>
        </surface>
    </collision>
    <xacro:cylinder_inertial mass="0.3" radius="0.1" length="0.05"/>
  </link>

  <link name="right_wheel">
    <visual>
        <geometry>
            <cylinder radius="0.1" length="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="1.57 0.0 0.0"/>
        <material name="black"/>     
    </visual>
    <collision>
        <geometry>
            <cylinder radius="0.1" length="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="1.57 0.0 0.0"/>
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
            <torsional>
              <ode/>
            </torsional>
          </friction>
        </surface>
    </collision>
    <xacro:cylinder_inertial mass="0.3" radius="0.1" length="0.05"/>
  </link>

  <link name="caster_wheel">
    <visual>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <material name="black"/>            
    </visual>
    <collision>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <surface>
          <friction>
            <ode>
              <mu>0.001</mu>
              <mu2>0.001</mu2>
            </ode>
            <torsional>
              <ode/>
            </torsional>
          </friction>
        </surface>
    </collision>
    <xacro:sphere_inertial mass="0.1" radius="0.05" />
  </link>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.50" rpy="0.0 0.0 0.0"/>
  </joint>

  
  <joint name="left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <origin xyz="0.1 0.23 -0.1" rpy="0 0.0 0.0"/>  
    <axis xyz="0.0 1.0 0.0"/>
    <dynamics damping="0.2" friction="1.0"/>
    <limit effort="100" velocity="1.0"/>
  </joint>

  <joint name="right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <origin xyz="0.1 -0.23 -0.1" rpy="0 0.0 0.0"/>  
    <axis xyz="0.0 1.0 0.0"/>
    <dynamics damping="0.2" friction="1.0"/>
    <limit effort="100" velocity="1.0"/>
  </joint>

  <joint name="caster_wheel_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_wheel"/>
    <origin xyz="-0.2 0.0 -0.15" rpy="0.0 0.0 0.0"/>
  </joint>

  <xacro:if value="$(arg use_arms)">
    <xacro:arms />
  </xacro:if>

  <xacro:if value="$(arg use_pantilt)">
    <xacro:pantilt parent="base_link" x="0.0" y="0.0" z="0.152" />
  </xacro:if>



  <!-- sensors -->
  
  <xacro:if value="$(arg use_imu)">
    <xacro:imu parent="base_link" x="-0.1" y="0.0" z="0.152" />
  </xacro:if>
  
  <xacro:if value="$(arg use_lidar)">
    <xacro:lidar parent="base_link" x="0.1" y="0.0" z="0.155" />
  </xacro:if>

  <xacro:property name="u_rgb" value="$(arg use_rgb)"/>
  <xacro:property name="u_rgbd" value="$(arg use_rgbd)"/>
  <xacro:property name="u_pantilt" value="$(arg use_pantilt)"/>

  <xacro:if value="${u_rgb and u_pantilt}">
    <xacro:rgb parent="pantilt_tip_link" x="0.1" y="0.0" z="0.5" />
  </xacro:if>

  <xacro:if value="${u_rgb and not u_pantilt}">
    <xacro:camera_base parent="base_link" x="0.0" y="0.0" z="0.152" />
    <xacro:rgb parent="pantilt_tip_link" x="0.1" y="0.0" z="0.5" />
  </xacro:if>

  <xacro:if value="${u_rgbd and u_pantilt}">
    <xacro:rgbd parent="pantilt_tip_link" x="0.1" y="0.0" z="0.5" />
  </xacro:if>

  <xacro:if value="${u_rgbd and not u_pantilt}">
    <xacro:camera_base parent="base_link" x="0.0" y="0.0" z="0.152" />
    <xacro:rgbd parent="pantilt_tip_link" x="0.1" y="0.0" z="0.5" />
  </xacro:if>


  <ros2_control name="GazeboSimSystem" type="system">
    <hardware>
      <plugin>gz_ros2_control/GazeboSimSystem</plugin>
    </hardware>
    <joint name="left_wheel_joint">
      <command_interface name="velocity">
        <param name="min">-10</param>
        <param name="max">10</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>
    <joint name="right_wheel_joint">
      <command_interface name="velocity">
        <param name="min">-10</param>
        <param name="max">10</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>

    <xacro:if value="$(arg use_arms)">

    <joint name="left_arm_joint">
      <command_interface name="effort">
        <param name="min">-10</param>
        <param name="max">10</param>
      </command_interface>
      <command_interface name="velocity">
        <param name="min">-1</param>
        <param name="max">1</param>
      </command_interface>
      <command_interface name="position">
        <param name="min">-100</param>
        <param name="max">100</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>
    <joint name="right_arm_joint">
      <command_interface name="effort">
        <param name="min">-10</param>
        <param name="max">10</param>
      </command_interface>
      <command_interface name="velocity">
        <param name="min">-1</param>
        <param name="max">1</param>
      </command_interface>
      <command_interface name="position">
        <param name="min">-100</param>
        <param name="max">100</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>
    
    </xacro:if>

    <xacro:if value="$(arg use_pantilt)">

    <joint name="pantilt_pan_joint">
      <command_interface name="effort">
        <param name="min">-10</param>
        <param name="max">10</param>
      </command_interface>
      <command_interface name="velocity">
        <param name="min">-1</param>
        <param name="max">1</param>
      </command_interface>
      <command_interface name="position">
        <param name="min">-100</param>
        <param name="max">100</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>
    <joint name="pantilt_tilt_joint">
      <command_interface name="effort">
        <param name="min">-10</param>
        <param name="max">10</param>
      </command_interface>
      <command_interface name="velocity">
        <param name="min">-1</param>
        <param name="max">1</param>
      </command_interface>
      <command_interface name="position">
        <param name="min">-100</param>
        <param name="max">100</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
    </joint>

    </xacro:if>

  </ros2_control>

  <gazebo>
    <!-- Joint state publisher -->
    <plugin filename="gz_ros2_control-system" name="gz_ros2_control::GazeboSimROS2ControlPlugin">
      <ros>
        <namespace>$(arg namespace)</namespace>
      </ros>
      <parameters>$(find marr_gz)/config/wheeled_controllers.yaml</parameters>
    </plugin>
  </gazebo>
  

  <xacro:if value="$(arg use_arms)">
    <gazebo>
       <plugin filename="gz-sim-detachable-joint-system"
               name="gz::sim::systems::DetachableJoint">
       <parent_link>left_arm_body</parent_link>
       <child_model>B1</child_model>
       <child_link>body</child_link>
       <detach_topic>/B1/detach</detach_topic>
       <attach_topic>/B1/attach</attach_topic>
       <output_topic>/B1/state</output_topic>
      </plugin>
    </gazebo>
  </xacro:if>


  
  
</robot>

